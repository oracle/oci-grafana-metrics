{
  "name": "oci-metrics-datasource",
  "private": true,
  "version": "6.0.1",
  "description": "Oracle Cloud Infrastructure Metrics Data Source for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix"
  },
  "author": "",
  "license": "UPL-1.0",
  "engines": {
    "node": ">=16"
  },
  "engineStrict": true,
  "devDependencies": {
    "@grafana/eslint-config": "^7.0.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.7",
    "@babel/core": "^7.24.1",
    "@babel/preset-react": "7.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "eslint-scope": "^5.1.1",
    "eslint-utils": "^2.1.0",
    "eslint-visitor-keys": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.7",
    "prunk": "^1.3.0",
    "@types/lodash": "4.14.195",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "@typescript-eslint/parser": "^5.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "babel-loader": "^8.0.2",
    "mocha": "6.x || 7.x || 8.x",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "eslint-plugin-deprecation": "^2.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "jest-environment-jsdom": "^29.5.0"
  },
  "dependencies": {
    "@apache-arrow/es5-cjs": "^4.0.1",
    "@babel/preset-env": "v7.24.1",
    "@emotion/css": "11.10.6",
    "@grafana/data": "^10.4.10",
    "@grafana/runtime": "^10.4.10",
    "@grafana/ui": "^10.4.10",
    "eslint": "^9.6.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@grafana/schema": "^10.4.10",
    "tslib": "2.5.3"
  },
  "resolutions": {
    "d3-color": "3.1.0"
  }
}
