    "@grafana/data": "latest",
    "@grafana/runtime": "latest",
    "@grafana/ui": "latest",    
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
